{
  "name": "yanbrain-api-client",
  "version": "1.0.0",
  "description": "YanBrain API wrapper for OpenAI, ElevenLabs, Wit.ai, and Runware - TypeScript Edition",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "yanbrain",
    "openai",
    "elevenlabs",
    "wit",
    "runware",
    "api",
    "typescript"
  ],
  "author": "Artem",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "openai": "^4.77.0",
    "@elevenlabs/elevenlabs-js": "^0.8.1",
    "@runware/sdk-js": "^0.3.0",
    "axios": "^1.6.2",
    "firebase-admin": "^12.6.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
