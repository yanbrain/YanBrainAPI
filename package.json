{
  "name": "yanbrain-api",
  "version": "1.0.0",
  "description": "YanBrain API wrapper for OpenAI, ElevenLabs, Wit.ai, and Runware - TypeScript Edition",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "token": "tsx src/testing/generateToken.ts",
    "test": "tsx src/testing/apiTests.ts",
    "test:health": "tsx src/testing/apiTests.ts health",
    "test:llm": "tsx src/testing/apiTests.ts llm",
    "test:tts": "tsx src/testing/apiTests.ts tts",
    "test:image": "tsx src/testing/apiTests.ts image",
    "test:embeddings": "tsx src/testing/embeddingTests.ts"
  },
  "author": "Artem",
  "license": "MIT",
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.28.0",
    "@runware/sdk-js": "^1.1.46",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^12.6.0",
    "multer": "^2.0.2",
    "officeparser": "^6.0.4",
    "openai": "^4.77.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
